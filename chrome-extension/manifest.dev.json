{
  "manifest_version": 3,
  "name": "Instalily Meeting Notes Helper",
  "version": "1.0.0",
  "key": "",
  "description": "Automatically sync meeting notes from Fathom, Circleback, Fireflies, and Zoom to your Instalily workspace",
  "permissions": [
    "cookies",
    "storage",
    "tabs",
    "identity",
    "notifications",
    "alarms",
    "windows",
    "scripting",
    "sidePanel"
  ],
  "host_permissions": [
    "https://*.fathom.video/*",
    "https://*.circleback.ai/*",
    "https://*.fireflies.ai/*",
    "https://*.zoom.us/*",
    "https://*.googleapis.com/*",
    "https://*.cloudfunctions.net/*",
    "https://*.run.app/*"
  ],
  "background": {
    "service_worker": "background/service-worker.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  "side_panel": {
    "default_path": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.fathom.video/*"],
      "js": ["content_scripts/fathom.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://*.circleback.ai/*"],
      "js": ["content_scripts/circleback.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://*.fireflies.ai/*"],
      "js": ["content_scripts/fireflies.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://*.zoom.us/*"],
      "js": ["content_scripts/zoom.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "oauth2": {
    "client_id": "778252380272-m677khkbppklvp7p8gifncneqafkhv3g.apps.googleusercontent.com",
    "scopes": [
      "openid",
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/datastore",
      "https://www.googleapis.com/auth/cloud-platform"
    ]
  }
}